    {
      "path": "dist/unify-fastify.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@types/node": "^18.0.0",
    "fastify": "^4.1.0",
    "unify-errors": "^1.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "wds": "^0.12.0"
  }
}
